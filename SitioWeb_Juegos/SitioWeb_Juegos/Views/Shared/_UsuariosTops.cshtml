@model IEnumerable<Servicios.Usuario>
    {}
<div class="card my-4">
    <h5 class="card-header">Tags</h5>
    <div class="card-body">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-6">
                    <ul class="list-unstyled mb-0">
                        <li>
                            <a href="#">@item.UserName</a>
                        </li>
                    </ul>
                </div>
                <div class="col-lg-6">
                    <ul class="list-unstyled mb-0">
                        <li>
                            <a href="#">@item.PuntajeTotal</a>
                        </li>
                    </ul>
                </div>
            }

        </div>
    </div>
</div>
