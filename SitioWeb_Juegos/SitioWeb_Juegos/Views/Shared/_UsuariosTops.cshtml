@model IEnumerable<Datos.AspNetUsers>

<div class="card my-4">
     <h5 class="card-header">Ranking Usuarios</h5>
     <div class="card-body"> 
         <ol>
             @foreach (var item in Model.OrderByDescending(x=> x.PuntajeTotal).Take(10))
             {

                 <li>
                 <a href="@Url.Action("Categorias","Posts",new { id = item.Id})">@item.UserName : </a> @item.PuntajeTotal
                </li>

             }
         </ol>
      </div>
</div>
